#include<iostream>
using namespace std;
int main()
{
    int i=0,j=1,b=1,a,c;
    cout<<"Enter last number of Fibonnaci series=";
    cin>>a;
    cout<<i<<"\n";
    cout<<j<<"\n";
    while(b<=a)
    {
        c=i+j;
        cout<<c;
        i=j;
        j=c;
        b++;
        cout<<"\n";
    }
    return 0;
}
