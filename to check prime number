#include <iostream>
using namespace std;
int main() {
    int check=0;
    int num;
    cout<<"Enter a number=";
    cin>>num;
    if(num ==0 || num==1)
    {
        cout<<num<<" is not a prime number.";
    }
    else{
        for(int i=2;i<num;i++)
        {
            if(num%i==0){
                cout<<num<<" is not a prime number.";
                check=1;
                break;
            }
        }
        if(check==0){
            cout<<num<<" is a prime number.";
            }
        }    
    return 0;
}
