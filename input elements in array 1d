#include <iostream>
using namespace std;
int main() {
    int n;
    cout << "Enter number of elements in array: ";
    cin >> n;
    int array[n];
    cout << "Enter the elements of the array:\n";
    for (int i = 0; i < n; i++) {
        cin >> array[i];
    }
    cout << "The elements of the array are: ";
    for (int i = 0; i < n; i++) {
        cout << array[i] << " ";
    }
    cout << "\n";
    return 0;
}
